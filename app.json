        "foregroundImage": "./assets/logo_icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.brendonavelino.mercadofacil"
    },
    "web": {
      "favicon": "./assets/logo_icon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "93c3a8d9-730d-4ea5-9bf5-a04b9894e832"
      }
    }
  }
